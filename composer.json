{
    "name": "carc1n0gen/weelnk",
    "description": "Small self hostable link shortening service written in PHP with the slim micro framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Evans",
            "email": "carsonwevans@gmail.com"
        }
    ],
    "require": {
        "carc1n0gen/short-link": "^1.1",
        "monolog/monolog": "^1.22",
        "slim/php-view": "^2.2",
        "slim/slim": "^3.7",
        "vlucas/phpdotenv": "^2.4",
        "doctrine/migrations": "^1.5",
        "cache/filesystem-adapter": "^1.0",
        "cache/array-adapter": "^1.0",
        "cache/predis-adapter": "^1.0",
        "cache/memcached-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "0.9.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Migrations\\": "migrations/"
        }
    }
}
